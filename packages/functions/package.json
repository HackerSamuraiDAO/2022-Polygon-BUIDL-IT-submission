{
  "name": "functions",
  "version": "0.0.1",
  "lisense": "NOLICENSE",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "lint": "run-p lint:*",
    "lint:prettier": "prettier '**/*.{json,md}' --ignore-path .gitignore --check",
    "lint:eslint": "eslint '**/*.{js,ts}' --ignore-path .gitignore",
    "fix": "run-p fix:*",
    "fix:prettier": "prettier '**/*.{json,md}' --ignore-path .gitignore --write",
    "fix:eslint": "yarn lint:eslint --fix",
    "dev": "ts-node-dev src/local.ts",
    "build": "rm -rf dist && tsc && npm-pack-zip"
  },
  "files": [
    "dist"
  ],
  "bundledDependencies": [
    "@vendia/serverless-express",
    "cors",
    "ethers",
    "express"
  ],
  "dependencies": {
    "@vendia/serverless-express": "^4.10.1",
    "aws-sdk": "^2.1199.0",
    "aws-serverless-express": "^3.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.9",
    "ethers-multicall": "^0.2.3",
    "express": "^4.18.1",
    "nft.storage": "^7.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.102",
    "@types/aws-serverless-express": "^3.3.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "18.6.4",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "npm-pack-zip": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.7.4"
  }
}
